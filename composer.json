{
    "name": "netstyler/cakephp-geoip",
    "description": "GeoIP Plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "GeoIp"
    ],
    "homepage": "https://github.com/netstyler/CakePHP-GeoIP-Plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Kr√§mer",
            "homepage": "http://florian-kraemer.net",
            "role": "Author"
        },
        {
            "name": "Falk Romano",
            "homepage": "http://www.netstyler.ch",
            "role": "Author"
        }
    ],
    "support": {
        "source": "https://github.com/netstyler/CakePHP-GeoIP-Plugin"
    },
    "scripts": {
        "post-install-cmd": [
            "php config/composer_post_install_script.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "Netstyler\\GeoIp\\": "src",
            "Netstyler\\GeoIp\\Test\\Fixture\\": "tests\\Fixture"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
            "Netstyler\\GeoIp\\Test\\": "tests"
        }
    },
    "require": {
        "cakephp/cakephp": "3.*",
        "pear/net_geoip": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "prefer-source": true
}
